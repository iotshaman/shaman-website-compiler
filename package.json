{
  "name": "shaman-website-compiler",
  "version": "4.0.15",
  "description": "Compile raw HTML, CSS and Javascript into the smallest possible, SEO friendly website.",
  "main": "dist/index.js",
  "typings": "public_api.d.ts",
  "private": true,
  "scripts": {
    "test": "nyc mocha",
    "build": "tsc",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Kyle Brown",
  "license": "ISC",
  "dependencies": {
    "clean-css": "^4.2.3",
    "fast-glob": "^3.2.4",
    "fs-extra": "^9.0.1",
    "gaze": "^1.1.3",
    "handlebars": "^4.7.7",
    "html-minifier": "^4.0.0",
    "inversify": "^5.0.1",
    "json-repo": "^1.2.2",
    "moment": "^2.29.1",
    "mongodb": "^3.6.2",
    "reflect-metadata": "^0.1.13",
    "sitemap": "^6.3.2",
    "uglify-es": "^3.3.9",
    "upath": "2.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/clean-css": "4.2.2",
    "@types/fs-extra": "^9.0.2",
    "@types/html-minifier": "4.0.0",
    "@types/mocha": "^8.0.3",
    "@types/mongodb": "^3.5.31",
    "@types/node": "^14.14.2",
    "@types/sinon": "9.0.8",
    "@types/uglify-es": "3.0.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.0",
    "ts-auto-mock": "^2.6.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js",
      "**/models/*.ts",
      "**/composition/*.ts",
      "**/data/*.ts"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  }
}
