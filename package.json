{
  "name": "shaman-website-compiler",
  "version": "4.0.24",
  "description": "Compile raw HTML, CSS and Javascript into the smallest possible, SEO friendly website.",
  "main": "dist/index.js",
  "typings": "public_api.d.ts",
  "private": true,
  "scripts": {
    "test": "nyc mocha",
    "build": "tsc",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Kyle Brown",
  "license": "ISC",
  "dependencies": {
    "clean-css": "^5.2.2",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.0",
    "gaze": "^1.1.3",
    "handlebars": "^4.7.7",
    "html-minifier": "^4.0.0",
    "inversify": "^6.0.1",
    "json-repo": "^1.2.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "mongodb": "^3.7.3",
    "reflect-metadata": "^0.1.13",
    "sitemap": "^7.1.0",
    "uglify-es": "^3.3.9",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/clean-css": "^4.2.5",
    "@types/fs-extra": "^9.0.13",
    "@types/html-minifier": "^4.0.2",
    "@types/mocha": "^9.1.0",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^14.18.9",
    "@types/sinon": "10.0.8",
    "@types/uglify-es": "3.0.0",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "ts-auto-mock": "^3.5.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "**/*.js",
      "**/models/*.ts",
      "**/composition/*.ts",
      "**/data/*.ts"
    ],
    "reporter": [
      "html",
      "lcov"
    ],
    "all": true
  }
}
