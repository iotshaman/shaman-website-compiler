{
  "name": "shaman-website-compiler",
  "version": "2.1.2",
  "description": "Generate website scaffolding in seconds. Compile to output with the CLI or compile to Express JS routes with Node JS. Built with SEO in mind.",
  "main": "scripts/index.js",
  "scripts": {
    "start": "tsc && node scripts/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "karma start --single-run",
    "test-watch": "karma start",
    "test-travis": "./node_modules/.bin/karma start karma.travis.conf.js --single-run"
  },
  "author": "Kyle Brown",
  "license": "ISC",
  "dependencies": {
    "clean-css": "^4.1.11",
    "fast-glob": "2.2.2",
    "fs-extra": "7.0.0",
    "gaze": "1.1.3",
    "handlebars": "4.0.11",
    "node-object-hash": "1.4.1",
    "promise": "8.0.1",
    "uglify-es": "3.3.9"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/jasmine": "2.8.8",
    "@types/node": "^8.0.53",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.5",
    "karma-coverage": "1.1.2",
    "karma-coveralls": "2.0.0",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "3.0.13",
    "typescript": "^2.6.1"
  }
}
